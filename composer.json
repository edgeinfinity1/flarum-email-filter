    "version": "1.0.0",
    "description": "Filter emails registering your Flarum site with whitelist, blacklist and even regular expressions.",
    "type": "flarum-extension",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NYU8\\FlarumEmailFilter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "NYU8",
            "email": "dev@nyu8.com"
        }
    ],
    "require": {
        "flarum/core": "^1.0",
        "fof/extend": "^1.0",
